                        Grid.Row="2"
                        Grid.Column="1"
                        HorizontalAlignment="Right"
                        VerticalAlignment="Bottom"
                        Stretch="Uniform">
                        <StackPanel Orientation="Horizontal">
                            <Image Source="{Binding Path=Player9[12]}" />
                            <Image Source="{Binding Path=Player9[11]}" />
                            <Image Source="{Binding Path=Player9[10]}" />
                        </StackPanel>
                    </Viewbox>
                    <Border
                        Grid.RowSpan="3"
                        Grid.Column="2"
                        Width="{Binding RelativeSource={RelativeSource Self}, Path=ActualHeight}"
                        Margin="10,0,0,5"
                        HorizontalAlignment="Right"
                        CornerRadius="20">
                        <Border.Background>
                            <ImageBrush ImageSource="{Binding Path=Player9[9]}" />
                        </Border.Background>
                    </Border>
                    <ProgressBar
                        Grid.Row="2"
                        Grid.Column="2"
                        Margin="10,0,0,0"
                        VerticalAlignment="Bottom"
                        Maximum="100"
                        Minimum="0"
                        Value="{Binding Path=Player9[8]}" />
                </Grid>
            </Border>
        </Grid>
        <TextBlock
            Grid.ColumnSpan="2"
            HorizontalAlignment="Center"
            VerticalAlignment="Center"
            FontSize="50"
            Foreground="White"
            Text="{Binding TestOutput, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />
    </Grid>
</UserControl>